{
	"name": "miscutils",
	"displayName": "miscutils",
	"description": "small DIY utils",
	"version": "0.0.1",
	"publisher": "LinkShen",
	"repository": {
		"type": "git",
		"url": "https://github.com/LinkShen/miscutils.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:codetree"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "miscutils",
					"title": "Misc Utils",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"miscutils": [
				{
					"id": "codetree",
					"name": "Code Tree"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "miscutils.codetree.addNode",
					"when": "view == codetree",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "miscutils.codetree.addNode",
					"when": "view == codetree",
					"group": "inline"
				},
				{
					"command": "miscutils.codetree.delNode",
					"when": "view == codetree",
					"group": "inline"
				}
			]
		},
		"commands": [
			{
				"command": "miscutils.codetree.addNode",
				"title": "Add Node",
				"icon":{"dark": "resources/add.svg","light": "resources/add.svg"},
				"category": "miscutils.codetree"
			},
			{
				"command": "miscutils.codetree.delNode",
				"title": "Del Node",
				"icon":{"dark": "resources/del.svg","light": "resources/del.svg"},
				"category": "miscutils.codetree"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
